{
	"name": "Symfony Development",
	"dockerComposeFile": ["docker-compose.yml"],
	"service": "www",
	"workspaceFolder": "/var/www/asc",
	"remoteEnv": {
		"APP_ENV": "dev",
		"APP_DEBUG": "1",
		"DATABASE_URL": "mysql://root:mourad@database:3306/asc"
	},
	"extensions": [
		"felixfbecker.php-debug",
		"dbaeumer.vscode-eslint",
		"editorconfig.editorconfig"
	],
	"settings": {
		"docker.host": "tcp://localhost:2375",
		"docker.remotePlatform": {"os": "linux"},
		"php.validate.enable": true,
		"phpunit.phpunit": "/var/www/asc/vendor/bin/phpunit",
		"symfony-vscode.devFolders": ["src", "config", "templates"]
	}
}
