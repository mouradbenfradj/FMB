<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
	<head>
		<meta charset="utf-8"/>
		<title>
			{% block title %}Logiciel ASC by TAC & SIDRATSOFT
			{% endblock %}
		</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description"/>
		<meta content="Coderthemes" name="author"/>
		<meta
		http-equiv="X-UA-Compatible" content="IE=edge"/>
		<!-- App favicon -->
		<link rel="shortcut icon" href="{{ asset('build/images/favicon.ico') }}">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<link href="{{ asset('build/css/bootstrap-creative-dark.min.css') }}" rel="stylesheet" type="text/css" id="bs-dark-stylesheet" disabled/>
			<link href="{{ asset('build/css/app-creative-dark.min.css') }}" rel="stylesheet" type="text/css" id="app-dark-stylesheet" disabled/>
		{% endblock %}
	</head>

	<body
		data-layout-mode="horizontal" data-layout='{"mode": "light", "width": "fluid", "menuPosition": "fixed", "topbar": {"color": "dark"}, "showRightSidebarOnPageLoad": true}'>

		<!-- Begin page -->
		<div
			id="wrapper">

			<!-- Topbar Start -->
			{% include '_topbar.html.twig' with {id: id} only %}
			<!-- end Topbar -->
			{% include '_topnav.html.twig' only %}
			<!-- ============================================================== -->
			<!-- Start Page Content here -->
			<!-- ============================================================== -->
				<div class="content-page"> <div
					class="content">
					<!-- Start Content-->
					<div
						class="container-fluid">

						<!-- end page title -->
						{% block body %}{% endblock %}
					</div>
					<!-- container -->

				</div>
				<!-- content -->

				<!-- Footer Start -->
				{% include '_footer.html.twig' only %}
				<!-- end Footer -->

			</div>

			<!-- ============================================================== -->
		<!-- End Page content -->
			<!-- ============================================================== -->


		</div>
		<!-- END wrapper -->

		<!-- Right Sidebar -->
		{% include '_sidebar.html.twig' only %}<!-- /Right-bar -->

		<!-- Right bar overlay--><div class="rightbar-overlay"> </div>

		<!-- Vendor js -->

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}

	</body>
</html>
