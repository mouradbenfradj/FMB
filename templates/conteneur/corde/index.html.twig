{% extends 'base.html.twig' %}

{% block title %}Corde index{% endblock %}

{% block body %}
    <h1>Corde index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for corde in cordes %}
            <tr>
                <td>{{ corde.id }}</td>
                <td>
                    <a href="{{ path('app_conteneur_corde_show', {'id': corde.id}) }}">show</a>
                    <a href="{{ path('app_conteneur_corde_edit', {'id': corde.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_conteneur_corde_new') }}">Create new</a>
{% endblock %}
