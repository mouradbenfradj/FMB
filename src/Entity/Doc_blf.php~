<?php

namespace App\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Doc_blf
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="App\Entity\Doc_blfRepository")
 */
class Doc_blf
{
    /**
     * @ORM\Id
     * @ORM\ManyToOne(targetEntity="App\Entity\Documents")
     * @ORM\JoinColumn(nullable=false)
     */
    private $refDoc;

    /**
     * @var string
     *
     * @ORM\Column(name="ref_doc_externe", type="string", length=32)
     */
    private $refDocExterne;

    /**
     * @ORM\ManyToOne(targetEntity="App\Entity\Stocks")
     */
    private $idStock;

}
