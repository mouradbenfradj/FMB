<body>
{% block header %}

    <!-- Navigation Bar-->
    <header id="topnav">
        {% block entete %}

            <div class="topbar-main">
                <div class="container">

                    <!-- Logo container-->
                    <div class="logo">
                        <a href="{{ path('oyster_pro28_homepage') }}" class="logo"><span><i class="md md-album"></i>YSTER PR<i
                                        class="md md-album"></i></span></a>
                    </div>
                    <!-- End Logo container-->
                    <div class="menu-extras">
                        <ul class="nav navbar-nav navbar-right pull-right">
                            <li class="dropdown navbar-c-items">
                                <a href="" class="dropdown-toggle waves-effect waves-light profile"
                                   data-toggle="dropdown"
                                   aria-expanded="true"><img
                                            src="{{ asset('bundles/oysterpro28/assets/images/users/sidratsoft.jpg') }}"
                                            alt="user-img"
                                            class="img-circle"> </a>
                                <ul class="dropdown-menu">
                                    <li><a href="{{ path('sonata_user_profile_show') }}"><i
                                                    class="ti-user text-custom m-r-10"></i> {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}
                                        </a></li>

                                    <li><a href="{{ path('sonata_admin_dashboard') }}"><i
                                                    class="ti-lock text-custom m-r-10"></i> Administration</a>
                                    </li>
                                    <li class="divider"></li>
                                    <li>
                                        <a href="{{ path('fos_user_security_logout') }}">
                                            <i class="ti-power-off text-danger m-r-10"></i> Logout</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                        <div class="menu-item">
                            <!-- Mobile menu toggle-->
                            <a class="navbar-toggle">
                                <div class="lines">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                </div>
                            </a>
                            <!-- End mobile menu toggle-->
                        </div>
                    </div>

                </div>
            </div>
        {% endblock %}
        {% block topnav %}
            <div class="navbar-custom">
                <div class="container">
                    <div id="navigation">
                        {{ knp_menu_render('OysterPro28Bundle:Builder:mainMenu') }}
                    </div>
                </div> <!-- end container -->
            </div> <!-- end navbar-custom -->
        {% endblock %}
    </header>
    <!-- End Navigation Bar-->
{% endblock %}
{% block body %}

    <div class="wrapper">
        <div class="container">
            {% block container %}
            {% endblock %}

            {% block footer %}

